{
  "manifest_version": 3,
  "name": "Tab Manager",
  "description": "Organize and manage your open tabs effectively!",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"

  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
